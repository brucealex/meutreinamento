img(class="icon-search" ng-click="coreEvents.toggle_search();")
// md-subheader.md-no-sticky Avatar with Secondary Action Icon
section(class="list-users"  ng-class="{'list-users': !coreEvents.search, 'list-users-open': coreEvents.search}")
	section(ng-repeat="person in [1,2,2,4,5,6] track by $index")
		img(class="img-responsive inline" alt="{{ person.name }}" holder='holder.js/43x43?auto=yes&random=yes&text=BRU')
		p(class="inline" ng-if="coreEvents.search") Bruno Alexandre
		img(class="icon-close" ng-if="coreEvents.search" ng-click="doSecondaryAction($event)")

