md-dialog(aria-label="Mango (Fruit)")
	section.edit-plan-wrap
		md-toolbar(md-theme="default") 
			.md-toolbar-tools
				h2 Editar Plano Treinamento
				span(flex="")
				md-button(ng-click="answer('not applicable')") Fechar
		md-dialog-content(style="margin: 16px; padding: 16px;")
				div(layout="row")
					div(flex="20", layout="", layout-align="center center")
						span.md-body-1 Duração
					md-slider#blue-slider.md-primary(flex="", ng-model="duracao", min="1", max="12", aria-label="blue")
					div(flex="10", layout="", layout-align="center center")
						input(type="number", ng-model="duracao", aria-label="blue", aria-controls="blue-slider")
				div(layout="row")
					div(flex="20", layout="", layout-align="center center")
						span.md-body-1 Frequência
					md-slider#blue-slider.md-primary(flex="", ng-model="frequencia", min="1", max="7", aria-label="blue")
					div(flex="10", layout="", layout-align="center center")
						input(type="number", ng-model="frequencia", aria-label="blue", aria-controls="blue-slider")
		.md-actions(layout="row")
			span(flex="")
			md-button.md-primary(ng-click="answer('not useful')")
				| Cancelar
			md-button.md-primary(ng-click="answer('useful')")
				| Salvar Alterações
