section.ds-serie-edit
	section.ds-serie-edit-wrap
		section.ds-serie-edit-select
			p selecionar
		section.ds-serie-edit-content
			section.ds-serie-edit-exercises
				p supino
				p polia
			section.ds-serie-edit-sliders
