							input(type="number", ng-model="repeticao", aria-label="blue", aria-controls="blue-slider")
				section.ds-serie-edit-slider-title
					span Repetições
				section.ds-serie-edit-slider-content
					div(layout="row")
						div(flex="20", layout="", layout-align="center center")
							span.md-body-1 Peso&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
						md-slider#blue-slider.md-primary(flex="",ng-disabled="true", ng-model="peso", min="0", max="280", aria-label="blue")
						div(flex="10", layout="", layout-align="center center")
							input(type="number", ng-model="peso", ng-disabled="true", aria-label="blue", aria-controls="blue-slider")
				section.ds-serie-edit-slider-title
					span Quilos

