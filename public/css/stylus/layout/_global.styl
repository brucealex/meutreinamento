.img-responsive
	max-width 100%
	height auto
	prefixer(border-radius, 50%)
.active
	border 2px solid palette('Green', 'A700')
	prefixer(box-sizing, border-box)
.square
	border-radius 8%
svg
	max-width 100%
md-input-container.md-default-theme label, 
	md-input-container.md-default-theme .md-placeholder
		color gray
button, svg, ng-md-icon, img, .admin-name
	&:focus
		outline 0
button.md-button
	text-transform none


